    }
   ],
   "source": [
    "# Extrahieren von Typ, Jahr, Kilometerstand, Getriebeart, Treibstofftyp, Postleitzahl und Location\n",
    "extracted = df['type_year_km_transmission_fuel'].str.extract(r'(\\w+)(\\d{2}.\\d{4})(\\d+\\.\\d+ km)([A-Z]\\w+)([A-Z]\\w+)(\\d{4}) \\(([^)]+)\\)')\n",
    "extracted.columns = ['Type', 'Year', 'Kilometers', 'Transmission', 'Fuel', 'PLZ', 'Location']\n",
    "\n",
    "# Ausgabe der ersten 5 extrahierten Datensätze\n",
    "print(extracted.head())"
   ]
